{
    // JDBC接続設定を個別に設定します.
    // ここでの名前は「ユニーク名」を設定してください.
    "name": {
        "name" :            "name",             // JDBC管理名を設定します.
        "driver":           "org.h2.Driver",    // JDBCドライバーパッケージ＋クラス名を設定します.
        "url":              "jdbc:h2:xxxx",     // 接続先URL、データベース名など.
        "user":             "",                 // 接続先ユーザ名.
        "password":         "",                 // 接続先パスワード.
        "readOnly":         false,              // リードオンリのみの接続ならtrue.
        "busyTimeout":      -1,                 // 問い合わせタイムアウト(0以下で無効).
        "transactionLevel": -1,                 // トランザクションレベル.
        "fetchSize":        -1,                 // フェッチサイズ(0以下で無効).
        "poolSize":         -1,                 // プーリングサイズ(0以下で最大プーリング数).
        "poolTimeout":      -1,                 // プーリングタイムアウト.
        "params":           {},                 // JDBCパラメータ.
        "urlType":          false,              // URLドライバパラメータ区切りタイプ.
                                                // [true] url + "?" + parmas + "&" ...
                                                // [false] url + ";" + params + ";" ...
        "urlParams":        {}                  // URLの後にドライバに追加するパラメータ.
    }
}